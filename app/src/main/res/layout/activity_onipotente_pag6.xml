<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3F51B5"
    android:backgroundTint="#3F51B5"
    tools:context=".epicuro.onipotentePag6">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/branco2"
        tools:context=".epicuro.pagina4">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="393dp"
            android:layout_height="282dp"
            android:layout_marginTop="68dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/epicuro400" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="277dp"
            android:layout_height="36dp"
            android:layout_marginTop="44dp"
            android:background="#3F51B5"
            android:backgroundTint="#3F51B5"
            android:text="Então ele não é Onipotente."
            android:textAlignment="center"
            android:textColor="#F4F1F1"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <TextView
            android:id="@+id/textView1311"
            android:layout_width="360dp"
            android:layout_height="176dp"
            android:layout_marginTop="36dp"
            android:text="Epicuro que sugere que Deus não é onipotente é uma forma de explicar por que o mal continua a existir no mundo, apesar da onisciência e da benevolência de Deus. Ela leva a uma reflexão profunda sobre a natureza de Deus, o significado de onipotência, e a razão pela qual o mal pode ser permitido ou inevitável em um mundo criado por um Deus que é, de outras formas, perfeito."
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12" />

        <TextView
            android:id="@+id/textView1411"
            android:layout_width="364dp"
            android:layout_height="52dp"
            android:layout_marginTop="8dp"
            android:text="Se Deus sabe da existência do mal e não consegue acabar com o mal, então ele não é todo poderoso."
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.553"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1311" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="393dp"
        android:layout_height="282dp"
        android:layout_marginTop="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/epicuro400" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="277dp"
        android:layout_height="36dp"
        android:layout_marginTop="44dp"
        android:background="#3F51B5"
        android:backgroundTint="#373943"
        android:text="Então ele não é Onipotente."
        android:textAlignment="center"
        android:textColor="#F4F1F1"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

</androidx.constraintlayout.widget.ConstraintLayout>